{
  "name": "availability-monitor",
  "version": "0.0.1",
  "description": "Desperately trying to purchase a card from Amazon",
  "main": "dist/app.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "tsc && node --trace-warnings dist/app.js",
    "test": "tsc && node --trace-warnings dist/test.js",
    "build": "tsc",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon --trace-warnings dist/app.js --verbose --ignore 'debug.log' --delay 1",
    "watch-ts": "tsc -w"
  },
  "author": "Pierpaolo Tommasi",
  "license": "MIT",
  "dependencies": {
    "2captcha": "^2.1.5-2",
    "axios": "^0.21.1",
    "chalk": "^4.1.1",
    "commander": "^7.2.0",
    "concurrently": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-proxy-agent": "^4.0.1",
    "http2-wrapper": "^2.0.5",
    "jsdom": "^16.5.3",
    "needle": "^2.6.0",
    "nodemon": "^2.0.7",
    "open": "^8.0.2",
    "play-sound": "^1.1.3",
    "proxy-chain": "^1.0.2",
    "puppeteer": "^8.0.0",
    "puppeteer-extra": "^3.1.18",
    "puppeteer-extra-plugin-adblocker": "^2.11.11",
    "puppeteer-extra-plugin-block-resources": "^2.2.9",
    "puppeteer-extra-plugin-stealth": "^2.7.6",
    "set-cookie-parser": "^2.4.8",
    "socket.io": "^4.0.0",
    "socket.io-client": "^4.0.0",
    "tslint": "^6.1.3",
    "twit": "^2.2.11",
    "typescript": "^4.2.3",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jsdom": "^16.2.10",
    "@types/needle": "^2.5.1",
    "@types/node": "^15.0.2",
    "@types/puppeteer": "^5.4.3",
    "@types/set-cookie-parser": "^2.4.0",
    "@types/socket.io": "^2.1.13",
    "@types/socket.io-client": "^1.4.36",
    "@types/twit": "^2.2.28",
    "@types/ws": "^7.4.0"
  }
}
